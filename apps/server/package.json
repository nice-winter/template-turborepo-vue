{
  "name": "@template-turborepo-vue/server",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "test": "echo \"OK\" && exit 0",
    "dev": "tsx watch --env-file=.env ./src/index.ts --nolazy",
    "start": "node --env-file=.env ./dist/index.js",
    "type-check": "tsc --noEmit",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "tsup"
  },
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@fastify/middie": "^9.0.3",
    "@template-turborepo-vue/math": "workspace:*",
    "fastify": "^5.3.0"
  },
  "devDependencies": {
    "@template-turborepo-vue/eslint-config": "workspace:*",
    "@template-turborepo-vue/tsconfig": "workspace:*",
    "@types/node": "~22.14.1",
    "esbuild-fix-imports-plugin": "^1.0.19",
    "eslint": "^9.24.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.3",
    "tslib": "^2.8.1",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "~5.8.3"
  }
}
